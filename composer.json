{
    "name": "bigfork/silverstripe-fail-whale",
    "type": "silverstripe-vendormodule",
    "description": "SilverStripe error pages, just without the pages.",
    "keywords": ["silverstripe", "error", "errorpage", "errors"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Loz Calver",
            "email": "lozcalver@bigfork.co.uk"
        }
    ],
    "require": {
        "silverstripe/framework": "^6",
        "silverstripe/siteconfig": "^6"
    },
    "autoload": {
        "psr-4": {
            "Bigfork\\SilverStripeFailWhale\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "autoload-dev": {
        "psr-4": {
            "Bigfork\\SilverStripeFailWhale\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
